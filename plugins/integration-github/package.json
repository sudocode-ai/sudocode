{
  "name": "@sudocode-ai/integration-github",
  "version": "0.1.18",
  "description": "GitHub integration plugin for sudocode using gh CLI",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest --run",
    "test:cli": "RUN_CLI_TESTS=true vitest --run",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "sudocode",
    "integration",
    "github",
    "plugin",
    "gh-cli"
  ],
  "author": "sudocode-ai",
  "license": "Apache-2.0",
  "peerDependencies": {
    "@sudocode-ai/types": "^0.1.18"
  },
  "devDependencies": {
    "@sudocode-ai/types": "^0.1.18",
    "@types/node": "^22.10.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
