{
  "name": "@sudocode-ai/types",
  "version": "0.1.7",
  "description": "TypeScript type definitions for sudocode",
  "types": "src/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.d.ts"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "default": "./dist/schema.js"
    },
    "./migrations": {
      "types": "./dist/migrations.d.ts",
      "default": "./dist/migrations.js"
    }
  },
  "scripts": {
    "build": "tsc"
  },
  "keywords": [
    "sudocode",
    "types",
    "typescript"
  ],
  "author": "sudocode AI",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sudocode-ai/sudocode.git",
    "directory": "types"
  },
  "homepage": "https://sudocode.ai",
  "bugs": {
    "url": "https://github.com/sudocode-ai/sudocode/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src/index.d.ts",
    "dist"
  ],
  "devDependencies": {
    "typescript": "^5.0.0"
  }
}
