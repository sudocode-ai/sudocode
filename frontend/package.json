{
  "name": "@sudocode-ai/local-ui",
  "version": "0.1.1",
  "description": "React frontend application for sudocode - UI for spec and issue management",
  "type": "module",
  "license": "Apache-2.0",
  "author": "sudocode AI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sudocode-ai/sudocode.git",
    "directory": "frontend"
  },
  "homepage": "https://sudocode.ai",
  "bugs": {
    "url": "https://github.com/sudocode-ai/sudocode/issues"
  },
  "keywords": [
    "sudocode",
    "ui",
    "react",
    "frontend",
    "spec-management",
    "issue-tracking"
  ],
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:verbose": "vitest --reporter=verbose --run",
    "test:quiet": "vitest --reporter=dot --run --silent",
    "test:summary": "vitest --reporter=basic --run --silent",
    "type-check": "tsc --noEmit"
  },
