# Port Configuration
# Backend server port (must match server's PORT env variable)
PORT=3000
BACKEND_PORT=3000
# Frontend dev server port
FRONTEND_PORT=3001

# API Configuration
# In development: Vite proxy forwards /api and /ws to backend on BACKEND_PORT
# In production: Frontend served from backend, so relative URLs work
VITE_API_URL=/api
VITE_WS_URL=/ws

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_TRACKING=false
